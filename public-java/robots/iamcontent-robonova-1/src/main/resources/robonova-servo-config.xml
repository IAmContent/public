<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <configFunctions>
        <!-- Pololu Maestro calibration for Hitec HSR4898HB servos -->
        <normalizingCalibrator>
            <defaultCalibrator>
                <valueConverter>
                    <toRange limit1="2432" limit2="9600"/>
                </valueConverter>
                <speedConverter>
                    <toRange limit1="0" limit2="200"/>
                </speedConverter>
                <accelerationConverter>
                    <toRange limit1="0" limit2="255"/>
                </accelerationConverter>
            </defaultCalibrator>
            <perChannelCalibrators/>
        </normalizingCalibrator>

        <channelTranslation>
            <mapping>
                <fromChannel xsi:type="servoId">LEFT_SHOULDER_FLEXOR</fromChannel>
                <toChannel xsi:type="xs:int">0</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">RIGHT_SHOULDER_FLEXOR</fromChannel>
                <toChannel xsi:type="xs:int">1</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">LEFT_SHOULDER_ABDUCTOR</fromChannel>
                <toChannel xsi:type="xs:int">2</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">RIGHT_SHOULDER_ABDUCTOR</fromChannel>
                <toChannel xsi:type="xs:int">3</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">LEFT_ELBOW_FLEXOR</fromChannel>
                <toChannel xsi:type="xs:int">4</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">RIGHT_ELBOW_FLEXOR</fromChannel>
                <toChannel xsi:type="xs:int">5</toChannel>
            </mapping>

            <mapping>
                <fromChannel xsi:type="servoId">LEFT_HIP_ABDUCTOR</fromChannel>
                <toChannel xsi:type="xs:int">14</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">RIGHT_HIP_ABDUCTOR</fromChannel>
                <toChannel xsi:type="xs:int">15</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">LEFT_HIP_FLEXOR</fromChannel>
                <toChannel xsi:type="xs:int">16</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">RIGHT_HIP_FLEXOR</fromChannel>
                <toChannel xsi:type="xs:int">17</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">LEFT_KNEE_FLEXOR</fromChannel>
                <toChannel xsi:type="xs:int">18</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">RIGHT_KNEE_FLEXOR</fromChannel>
                <toChannel xsi:type="xs:int">19</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">LEFT_ANKLE_PLANTARFLEXOR</fromChannel>
                <toChannel xsi:type="xs:int">20</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">RIGHT_ANKLE_PLANTARFLEXOR</fromChannel>
                <toChannel xsi:type="xs:int">21</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">LEFT_ANKLE_INVERTER</fromChannel>
                <toChannel xsi:type="xs:int">22</toChannel>
            </mapping>
            <mapping>
                <fromChannel xsi:type="servoId">RIGHT_ANKLE_INVERTER</fromChannel>
                <toChannel xsi:type="xs:int">23</toChannel>
            </mapping>
        </channelTranslation>

        <!-- Tuning of the individual Robonova servos -->
        <tuningCalibrator>
            <defaultCalibrator>
                <valueConverter>
                    <toRange limit1="0" limit2="1"/>
                </valueConverter>
            </defaultCalibrator>
            <perChannelCalibrators>
                <calibrator>
                    <channel xsi:type="servoId">RIGHT_SHOULDER_FLEXOR</channel>
                    <valueConverter>
                        <toRange limit1="1" limit2="0"/>
                    </valueConverter>
                </calibrator>
                <calibrator>
                    <channel xsi:type="servoId">RIGHT_SHOULDER_ABDUCTOR</channel>
                    <valueConverter>
                        <toRange limit1="1" limit2="0"/>
                    </valueConverter>
                </calibrator>
                <calibrator>
                    <channel xsi:type="servoId">LEFT_ELBOW_FLEXOR</channel>
                    <valueConverter>
                        <toRange limit1="1" limit2="0"/>
                    </valueConverter>
                </calibrator>
                <calibrator>
                    <channel xsi:type="servoId">LEFT_HIP_ABDUCTOR</channel>
                    <valueConverter>
                        <toRange limit1="0.6" limit2="0" notes="Physical stop on full adduction"/>
                    </valueConverter>
                </calibrator>
                <calibrator>
                    <channel xsi:type="servoId">RIGHT_HIP_ABDUCTOR</channel>
                    <valueConverter>
                        <toRange limit1="0.4" limit2="1" notes="Physical stop on full adduction"/>
                    </valueConverter>
                </calibrator>
                <calibrator>
                    <channel xsi:type="servoId">RIGHT_HIP_FLEXOR</channel>
                    <valueConverter>
                        <toRange limit1="1" limit2="0"/>
                    </valueConverter>
                </calibrator>
                <calibrator>
                    <channel xsi:type="servoId">LEFT_KNEE_FLEXOR</channel>
                    <valueConverter>
                        <toRange limit1="0.05" limit2="0.95" notes="Hard stop and some L/R asymmetry"/>
                    </valueConverter>
                </calibrator>
                <calibrator>
                    <channel xsi:type="servoId">RIGHT_KNEE_FLEXOR</channel>
                    <valueConverter>
                        <toRange limit1="0.98" limit2="0.08" notes="Hard stop and some L/R asymmetry"/>
                    </valueConverter>
                </calibrator>
                <calibrator>
                    <channel xsi:type="servoId">LEFT_ANKLE_PLANTARFLEXOR</channel>
                    <valueConverter>
                        <toRange limit1="0.15" limit2="1" notes="Hard stops"/>
                    </valueConverter>
                </calibrator>
                <calibrator>
                    <channel xsi:type="servoId">RIGHT_ANKLE_PLANTARFLEXOR</channel>
                    <valueConverter>
                        <toRange limit1="0.85" limit2="0" notes="Hard stops"/>
                    </valueConverter>
                </calibrator>
                <calibrator>
                    <channel xsi:type="servoId">LEFT_ANKLE_INVERTER</channel>
                    <valueConverter>
                        <toRange limit1="0.55" limit2="0.25" notes="Hard stops"/>
                    </valueConverter>
                </calibrator>
                <calibrator>
                    <channel xsi:type="servoId">RIGHT_ANKLE_INVERTER</channel>
                    <valueConverter>
                        <toRange limit1="0.45" limit2="0.75" notes="Hard stops"/>
                    </valueConverter>
                </calibrator>
            </perChannelCalibrators>
         </tuningCalibrator>
    </configFunctions>
</config>
